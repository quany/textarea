{
  "name": "@icoding/textarea",
  "private": false,
  "version": "0.1.1",
  "description": "iCoding ai for textarea",
  "scripts": {
    "build": "tsup --clean",
    "dev": "tsup --watch --no-splitting",
    "test": "jest",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf .next",
    "link:global": "pnpm link --global",
    "unlink:global": "pnpm unlink --global"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/quany/textarea",
  "repository": {
    "type": "git",
    "url": "https://github.com/quany/textarea.git"
  },
  "keywords": [
    "react",
    "nextjs",
    "nodejs",
    "ai",
    "assistant",
    "javascript",
    "automation",
    "textarea"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/index.css"
  },
  "types": "./dist/index.d.ts",
  "author": "quany@live.com",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@copilotkit/react-core": "^1.1.1",
    "@copilotkit/react-textarea": "^1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.4",
    "@types/react-syntax-highlighter": "^15.5.7",
    "eslint": "^8.56.0",
    "jest": "^29.6.4",
    "postcss": "^8.4.20",
    "postcss-prefix-selector": "^1.16.1",
    "react": "^18.2.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.1",
    "tsup": "^6.7.0",
    "typescript": "^5.2.3",
    "eslint-config-custom": "1.1.0",
    "tsconfig": "1.1.0"
  }
}
