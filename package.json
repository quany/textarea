{
  "name": "@icoding/textarea",
  "private": false,
  "version": "0.1.9",
  "description": "iCoding ai for textarea",
  "scripts": {
    "dev": "tsup --watch --no-splitting",
    "test": "jest",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf .next",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm run prettier && npm run lint",
    "link:global": "pnpm link --global",
    "unlink:global": "pnpm unlink --global"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/quany/textarea",
  "repository": {
    "type": "git",
    "url": "https://github.com/quany/textarea.git"
  },
  "keywords": [
    "react",
    "nextjs",
    "nodejs",
    "ai",
    "assistant",
    "javascript",
    "automation",
    "textarea"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "author": "quany@live.com",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@copilotkit/react-core": "1.1.1",
    "@copilotkit/react-textarea": "1.1.1"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.4",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.6.4",
    "postcss": "^8.4.20",
    "postcss-prefix-selector": "^1.16.1",
    "prettier": "^3.3.3",
    "react": "^18.2.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.1",
    "tsconfig": "1.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.2.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
